{
  "schemaVersion": 1,
  "id": "eechelons",
  "version": "${version}",

  "name": "Enemy Echelons",
  "description": "Enemy Echelons is a mod that assigns levels and attribute modifications to mobs based on the world height range that they spawn at.",
  "authors": [
    "gottsch"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/enemy-echelons",
    "sources": "https://github.com/gottsch/gottsch-minecraft-EnemyEchelons"
  },

  "license": "LGPL-3.0-or-later",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "mod.gottsch.fabric.eechelons.EEchelons"
    ],
    "client": [
      "mod.gottsch.fabric.eechelons.core.setup.ClientSetup"
    ]
  },
  "mixins": [
    "eechelons.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.14",
    "fabric-api": "*",
    "minecraft": "~1.19.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
